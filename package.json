{
    "name": "mevn-stack",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "14.2.0"
    },
    "scripts": {
        "start": "set NODE_ENV=production&&node prod-server/index.js",
        "serve": "vue-cli-service serve",
        "build": "set NODE_ENV=production&&babel dev-server --out-dir prod-server && vue-cli-service build ",
        "lint": "vue-cli-service lint",
        "dev": "concurrently \"set NODE_ENV=development\" \"babel dev-server --out-dir prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\"",
        "prod": "babel dev-server --out-dir prod-server"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.19.0",
        "core-js": "^3.6.5",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.27.0",
        "mongoose": "^5.9.25",
        "vue": "^2.6.11",
        "vue-router": "^3.2.0",
        "vuex": "^3.4.0",
        "@babel/cli": "^7.10.5",
        "@babel/preset-env": "^7.10.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.4.0",
        "@vue/cli-plugin-eslint": "~4.4.0",
        "@vue/cli-plugin-router": "~4.4.0",
        "@vue/cli-plugin-vuex": "~4.4.0",
        "@vue/cli-service": "~4.4.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "babel-eslint": "^10.1.0",
        "bootstrap": "^4.5.0",
        "concurrently": "^5.2.0",
        "cors": "^2.8.5",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "jquery": "^3.5.1",
        "morgan": "^1.10.0",
        "popper.js": "^1.16.1",
        "prettier": "^1.19.1",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
